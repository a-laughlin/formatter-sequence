{
  "name": "formatter-sequence",
  "displayName": "formatter-sequence",
  "publisher": "alaughlin",
  "description": "runs formatters in sequence",
  "version": "0.0.1",
  "repository": "https://github.com/a-laughlin/formatter-sequence",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "formatter-sequence",
        "properties": {
          "formatter-sequence": {
            "type": "array",
            "default": [],
            "description": "Array of formatter names. See valid formatters in autocomplete for settings > editor.defaultFormatter."
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:package": "vsce package",
    "vscode:prepublish": "npm run compile",
    "vscode:publish": "vsce publish",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@vscode/vsce": "^2.24.0"
  }
}
